{
  "template":   {
    "displayName": "Template",
    "remoteName": "@alfresco/apa-template",
    "exposedModule": "./Template",
    "componentName": "AppTemplateComponent",
    "options": {
      "title": "Workspace",
      "headerColor": "default"
    }
  },
  "widgets": [],
  "components": [
    {
      "displayName": "File browser",
      "remoteName": "@extensible-angular-app/content",
      "exposedModule": "./Files",
      "componentName": "FilesModule",
      "options": {
        "protected": true,
        "route": "files",
        "title": "Files",
        "icon": "view_list",
        "folderId": "<root>"
      }
    },
    {
      "displayName": "Process browser",
      "remoteName": "@extensible-angular-app/process",
      "exposedModule": "./Processes",
      "componentName": "ProcessComponent",
      "standalone": true,
      "options": {
        "protected": true,
        "route": "processes",
        "title": "Processes",
        "icon": "blur_circular",
        "processCategory": "<my>"
      }
    }
  ],
  "plugins": []
}
